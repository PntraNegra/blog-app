<div class="container">
  <p style="color: green"><%= notice %></p>

  <%= render @user %>

  <div class="mb-3">
    <%= link_to "Edit this user", edit_user_path(@user) %> |
    <%= link_to "Back to users", users_path %>

    <%= button_to "Delete Account", @user, method: :delete, class: "btn btn-outline-danger" %>
  </div>
</div>